{
  "name": "teleme-analytics-sveltekit",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite dev --port 9999",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "pnpm lint && pnpm format:check && pnpm check",
    "approve-builds": "pnpm approve-builds"
  },
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^3.6.0",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^3.4.3",
    "svelte-eslint-parser": "^1.1.3",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.4.1",
    "typescript": "^5.8.3",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "axios": "^1.4.0",
    "dompurify": "^3.0.4",
    "dotenv": "^16.1.4",
    "jose": "^5.0.0",
    "jsonwebtoken": "^9.0.0",
    "marked": "^9.0.0",
    "uuid": "^11.1.0"
  },
  "type": "module"
}
